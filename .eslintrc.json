{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "indent": "off",
    "linebreak-style": "off",
    "max-len": "off",
    "no-underscore-dangle": "off",
    "comma-dangle": "off",
    "object-curly-newline": "off",
    "no-unused-vars": "off",
    "no-restricted-globals": "off",
    "no-undef": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
